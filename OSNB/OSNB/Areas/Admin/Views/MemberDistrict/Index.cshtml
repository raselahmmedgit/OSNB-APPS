        <a href="@Url.Action("Add", "MemberDistrict")" id="lnkAddMemberDistrict" class="btn btn-primary">
            <icon class="icon-plus-sign icon-white"></icon>
            Add District</a>
    </div>
    <div class="row-fluid" id="memberDistrictDataTableZone">
        <table id="memberDistrictDataTable" class="table table-bordered table-hover">
            <thead>
                <tr>
                    <th>
                        Zones
                    </th>
                    <th>
                        DistrictName
                    </th>
                    <th>
                        Actions
                    </th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
    <div id="addMemberDistrictDialog" title="Add District">
    </div>
    <div id="editMemberDistrictDialog" title="Edit District">
    </div>
    <div id="deleteMemberDistrictDailog" title="Delete District">
    </div>
    <div id="detailsMemberDistrictDialog" title="Details District">
    </div>
</div>
<script src="@Url.Content("~/Scripts/App/memberdistrict-jq.js")" type="text/javascript"></script>
