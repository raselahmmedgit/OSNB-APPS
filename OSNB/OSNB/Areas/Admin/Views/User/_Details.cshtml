@model OSNB.ViewModels.UserViewModel
<div id="userDetailsPage">
    <!-- UserName -->
    <div class="row-fluid">
        <div class="span4">
            <div class="control-label">
                @Html.LabelFor(model => model.UserName)
            </div>
        </div>
        <div class="span8">
            @Html.DisplayFor(x => x.UserName)
        </div>
    </div>
    <!-- Email -->
    <div class="row-fluid">
        <div class="span4">
            <div class="control-label">
                @Html.LabelFor(model => model.Email)
            </div>
        </div>
        <div class="span8">
            @Html.DisplayFor(x => x.Email)
        </div>
    </div>
</div>
