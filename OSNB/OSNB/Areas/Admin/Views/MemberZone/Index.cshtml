@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div id="memberZoneIndexPage">
    <div class="row-fluid">
        <a href="@Url.Action("Add", "MemberZone")" id="lnkAddMemberZone" class="btn btn-primary">
            <icon class="icon-plus-sign icon-white"></icon>
            Add Zone</a>
    </div>
    <div class="row-fluid" id="memberZoneDataTableZone">
        <table id="memberZoneDataTable" class="table table-bordered table-hover">
            <thead>
                <tr>
                    <th>
                        Zone Name
                    </th>
                    <th>
                        Location X
                    </th>
                    <th>
                        Location Y
                    </th>
                    <th>
                        District Name
                    </th>
                    <th>
                        Actions
                    </th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
    <div id="addMemberZoneDialog" title="Add Zone">
    </div>
    <div id="editMemberZoneDialog" title="Edit Zone">
    </div>
    <div id="deleteMemberZoneDailog" title="Delete Zone">
    </div>
    <div id="detailsMemberZoneDialog" title="Details Zone">
    </div>
</div>
<script src="@Url.Content("~/Scripts/App/memberzone-jq.js")" type="text/javascript"></script>
